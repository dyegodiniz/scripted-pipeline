#!/usr/bin/env groovy



node {
    stage('build') {
        echo 'hello'
        sh 'ls -l'
        sh 'pwd'
    }
    stage('test') {
        echo 'hello'
    }
    stage('deploy') {
        echo 'hello'
    }
    stage('print all') {
        println("valor do this:")
        println(this)
        println("valor do env:")
        println(env)

    }
}





